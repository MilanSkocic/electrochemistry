@article{ribiere2023,
  title = {Multi-{{Scale Characterization}} of the {{Inner Surface}} of as-{{Received Steam Generator Tubes}} and {{Correlation}} with the {{Ni Release}} in {{Primary Water}}},
  author = {Ribière, Nathan and Engler, Nathalie and Brimbal, Daniel and Skocic, Milan and Andrieu, Éric and Blanc, Christine and Laffont, Lydia},
  date = {2023-07-01},
  journaltitle = {Corrosion Science},
  shortjournal = {Corrosion Science},
  volume = {218},
  pages = {111205},
  issn = {0010-938X},
  doi = {10.1016/j.corsci.2023.111205},
  url = {https://www.sciencedirect.com/science/article/pii/S0010938X23002470},
  abstract = {The inner surface of two as-received steam generator (SG) tubes made of nickel base alloy 690 was analyzed to explain the differences in nickel release obtained for the tubes in a loop simulating the primary environment of pressurized water reactors. The roughness, microstructure and chemical heterogeneities were characterized; differences that could explain the specific behavior of the two tubes in primary water were noted. In particular, the presence of alumina particles at the surface of SG tubes influences the nickel release: their dissolution in the primary coolant induces a change in roughness and a local breakdown of the passive film.},
  keywords = {A. Nickel,B.SEM,C. Inclusion,C. Interfaces,C. Oxidation,Ni Release,PhD Nathan Ribière,R&D}
}

@article{ribiere2024,
  title = {An {{XPS}} and {{TEM Study}} of the {{Composition}} and {{Structure}} of {{Native Oxides}} on the {{Inner Surface}} of {{As-Received Ni Base Alloy Steam Generator Tubes}}},
  author = {Ribière, Nathan and Esvan, Jérôme and Engler, Nathalie and Brimbal, Daniel and Skocic, Milan and Andrieu, Éric and Blanc, Christine and Laffont, Lydia},
  date = {2024-05-01},
  journaltitle = {Applied Surface Science},
  shortjournal = {Applied Surface Science},
  volume = {654},
  pages = {159514},
  issn = {0169-4332},
  doi = {10.1016/j.apsusc.2024.159514},
  url = {https://www.sciencedirect.com/science/article/pii/S0169433224002277},
  abstract = {Ni based alloys steam generator (SG) tubes in pressurized water reactors may suffer generalized corrosion and Ni release in primary water. The processes of corrosion and oxidation are strongly dependent on the surface properties. In particular, the chemical composition and structure of the native oxide films present at the surface of the SG tubes are crucial and improving the passive properties of the surface could contribute to reduce the corrosion damage and Ni release. The native oxides on the inner surface of two as-received Ni base alloy SG tubes were investigated using two main techniques, namely X-ray photoemission spectroscopy and transmission electron microscopy. The chemical elements present in the native film for the two tubes were identified and the structure and composition of the native film at the nanoscale was determined. A duplex oxide structure was identified for both samples, but with differences in the chemical composition of the inner and outer oxide layer. Chemical heterogeneities such as alumina, Ti oxides and Cr oxides were also detected; they are considered to play a role in the evolution of the surface exposed to primary water. The two techniques used allowed a multi-scale description of the surface and led to complementary results.},
  keywords = {Native surface,Ni Release,Nickel,Oxide,PhD Nathan Ribière,PWR,R&D,TEM,XPS}
}

